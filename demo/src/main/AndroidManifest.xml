<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.gncy2013.smartlock.demo" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="io.github.gncy2013.smartlock.demo.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="io.github.gncy2013.smartlock.activity.LockPatternActivity"
            android:label="@string/activity_pattern_lock"
            android:screenOrientation="portrait"
            android:theme="@style/LockActivity" >
        </activity>
        <activity
            android:name="io.github.gncy2013.smartlock.activity.SetPatternActivity"
            android:label="@string/activity_setting"
            android:screenOrientation="portrait"
            android:theme="@style/LockActivity" >
        </activity>
        <activity
            android:name="io.github.gncy2013.smartlock.activity.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="io.github.gncy2013.smartlock.demo.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="io.github.gncy2013.smartlock.demo.MainActivity" />
        </activity>
    </application>

</manifest>
